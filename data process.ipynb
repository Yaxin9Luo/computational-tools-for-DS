   "source": [
    "# upload stopwords txt\n",
    "#gist_file = open(root+\"/english\", \"r\")\n",
    "content = gist_file.read()\n",
    "stopwords = content.split(\"\\n\")\n",
    "gist_file.close()"
   ]
